{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport \"core-js/modules/es.iterator.constructor.js\";\nimport \"core-js/modules/es.iterator.for-each.js\";\nimport { ref } from \"vue\";\nimport clothes from \"@/collections/clothes\";\n\n// const clothes = clothes\n\nexport default {\n  __name: 'SelectedThings',\n  setup(__props, {\n    expose: __expose\n  }) {\n    __expose();\n    const outwear = ref([{\n      \"id\": 11,\n      \"name\": \"Jacket 1\"\n    }, {\n      \"id\": 12,\n      \"name\": \"Jacket 2\"\n    }, {\n      \"id\": 13,\n      \"name\": \"Jacket 3\"\n    }, {\n      \"id\": 14,\n      \"name\": \"Jacket 4\"\n    }, {\n      \"id\": 15,\n      \"name\": \"Hoodie 1\"\n    }, {\n      \"id\": 16,\n      \"name\": \"Hoodie 2\"\n    }, {\n      \"id\": 17,\n      \"name\": \"Hoodie 3\"\n    }, {\n      \"id\": 18,\n      \"name\": \"Hoodie 4\"\n    }]);\n    const selectedClothes = ref([]);\n    const selectedOutwear = ref(null);\n\n    // Добавление вещи в блок выбранных вещей\n    const selectClothes = item => {\n      if (selectedClothes.value.length === 6) {\n        return;\n      }\n      const [selectedItem] = clothes.value.splice(item, 1);\n      selectedClothes.value.push(selectedItem);\n    };\n\n    // Удаление вещи при нажатии на нее\n    const removeSelectedClothes = item => {\n      const [selectedItem] = selectedClothes.value.splice(item, 1);\n      clothes.value.push(selectedItem);\n      clothes.value.sort((a, b) => a.id - b.id);\n    };\n\n    // Удаление всех выбранных вещей\n    const removeAllSelectedClothes = () => {\n      selectedClothes.value.forEach(el => {\n        clothes.value.push(el);\n        clothes.value.sort((a, b) => a.id - b.id);\n      });\n      selectedClothes.value = [];\n    };\n    const selectOutwear = item => {\n      if (selectedOutwear.value) {\n        outwear.value.push(selectedOutwear.value);\n      }\n      const index = outwear.value.findIndex(i => i.id === item.id);\n      if (index !== -1) {\n        outwear.value.splice(index, 1);\n      }\n      selectedOutwear.value = item;\n      outwear.value.sort((a, b) => a.id - b.id);\n    };\n    const removeSelectedOutwear = () => {\n      outwear.value.push(selectedOutwear.value);\n      outwear.value.sort((a, b) => a.id - b.id);\n      selectedOutwear.value = null;\n    };\n    const __returned__ = {\n      outwear,\n      selectedClothes,\n      selectedOutwear,\n      selectClothes,\n      removeSelectedClothes,\n      removeAllSelectedClothes,\n      selectOutwear,\n      removeSelectedOutwear,\n      ref,\n      get clothes() {\n        return clothes;\n      }\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n};","map":{"version":3,"names":["ref","clothes","outwear","selectedClothes","selectedOutwear","selectClothes","item","value","length","selectedItem","splice","push","removeSelectedClothes","sort","a","b","id","removeAllSelectedClothes","forEach","el","selectOutwear","index","findIndex","i","removeSelectedOutwear"],"sources":["/Users/kirill/WebstormProjects/vacancy-test-apps/thing-app/src/components/SelectedThings.vue"],"sourcesContent":["<template>\n  <div class=\"w-75 ma-0 ma-auto\">\n    <div class=\"d-flex ga-5\">\n      <div class=\"w-50 border-solid mb-2 pa-2\">\n        <div class=\"d-flex flex-wrap ga-2\">\n          <div class=\"clothes-block__item\" v-for=\"(item, index) in selectedClothes\" :key=\"index\" @click=\"removeSelectedClothes(item)\">{{item.name}}</div>\n        </div>\n        Выбрано: {{selectedClothes.length}} / 6 <button class=\"cursor-pointer\" @click=\"removeAllSelectedClothes\">Очистить</button>\n      </div>\n\n      <div class=\"w-50 border-solid mb-2 pa-2\">\n        <div class=\"d-flex flex-wrap ga-2\">\n          <div v-if=\"selectedOutwear\" class=\"clothes-block__item\" @click=\"removeSelectedOutwear\">{{selectedOutwear.name}}</div>\n        </div>\n      </div>\n    </div>\n\n    <div class=\"d-flex justify-space-between ga-5\">\n      <div class=\"clothes-block\">\n        <div class=\"clothes-block__item\" v-for=\"(item, index) in clothes\" :key=\"index\" @click=\"selectClothes(item)\">\n          {{item.name}}\n        </div>\n      </div>\n      <div class=\"clothes-block\">\n        <div class=\"clothes-block__item\" v-for=\"(item, index) in outwear\" :key=\"index\" @click=\"selectOutwear(item)\">\n          {{item.name}}\n        </div>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script setup>\nimport {ref} from \"vue\";\nimport clothes from \"@/collections/clothes\";\n\n// const clothes = clothes\n\nconst outwear = ref([\n  {\n    \"id\": 11,\n    \"name\": \"Jacket 1\"\n  },\n  {\n    \"id\": 12,\n    \"name\": \"Jacket 2\"\n  },\n  {\n    \"id\": 13,\n    \"name\": \"Jacket 3\"\n  },\n  {\n    \"id\": 14,\n    \"name\": \"Jacket 4\"\n  },\n  {\n    \"id\": 15,\n    \"name\": \"Hoodie 1\"\n  },\n  {\n    \"id\": 16,\n    \"name\": \"Hoodie 2\"\n  },\n  {\n    \"id\": 17,\n    \"name\": \"Hoodie 3\"\n  },\n  {\n    \"id\": 18,\n    \"name\": \"Hoodie 4\"\n  }\n])\n\nconst selectedClothes = ref([])\nconst selectedOutwear = ref(null)\n\n// Добавление вещи в блок выбранных вещей\nconst selectClothes = (item) => {\n  if(selectedClothes.value.length === 6) {\n    return\n  }\n  const [selectedItem] = clothes.value.splice(item, 1)\n  selectedClothes.value.push(selectedItem)\n}\n\n// Удаление вещи при нажатии на нее\nconst removeSelectedClothes = (item) => {\n  const [selectedItem] = selectedClothes.value.splice(item, 1)\n  clothes.value.push(selectedItem)\n  clothes.value.sort((a, b) => a.id - b.id)\n}\n\n// Удаление всех выбранных вещей\nconst removeAllSelectedClothes = () => {\n  selectedClothes.value.forEach(el => {\n    clothes.value.push(el)\n    clothes.value.sort((a, b) => a.id - b.id)\n  })\n  selectedClothes.value = []\n}\n\nconst selectOutwear = (item) => {\n  if (selectedOutwear.value) {\n    outwear.value.push(selectedOutwear.value);\n  }\n  const index = outwear.value.findIndex(i => i.id === item.id);\n  if (index !== -1) {\n    outwear.value.splice(index, 1);\n  }\n  selectedOutwear.value = item;\n  outwear.value.sort((a, b) => a.id - b.id);\n};\n\nconst removeSelectedOutwear = () => {\n  outwear.value.push(selectedOutwear.value)\n  outwear.value.sort((a, b) => a.id - b.id);\n  selectedOutwear.value = null\n}\n</script>\n\n<style lang=\"scss\">\n.clothes-block {\n  display: flex;\n  flex-wrap: wrap;\n  padding: 20px;\n  border: 1px solid black;\n  row-gap: 20px;\n  column-gap: 20px;\n  width: 50%;\n\n  &__item {\n    cursor: pointer;\n    padding: 20px;\n    border: 1px solid black;\n    height: fit-content;\n  }\n}\n</style>"],"mappings":";;;AAiCA,SAAQA,GAAG,QAAO,KAAK;AACvB,OAAOC,OAAO,MAAM,uBAAuB;;AAE3C;;;;;;;;IAEA,MAAMC,OAAO,GAAGF,GAAG,CAAC,CAClB;MACE,IAAI,EAAE,EAAE;MACR,MAAM,EAAE;IACV,CAAC,EACD;MACE,IAAI,EAAE,EAAE;MACR,MAAM,EAAE;IACV,CAAC,EACD;MACE,IAAI,EAAE,EAAE;MACR,MAAM,EAAE;IACV,CAAC,EACD;MACE,IAAI,EAAE,EAAE;MACR,MAAM,EAAE;IACV,CAAC,EACD;MACE,IAAI,EAAE,EAAE;MACR,MAAM,EAAE;IACV,CAAC,EACD;MACE,IAAI,EAAE,EAAE;MACR,MAAM,EAAE;IACV,CAAC,EACD;MACE,IAAI,EAAE,EAAE;MACR,MAAM,EAAE;IACV,CAAC,EACD;MACE,IAAI,EAAE,EAAE;MACR,MAAM,EAAE;IACV,EACD;IAED,MAAMG,eAAe,GAAGH,GAAG,CAAC,EAAE;IAC9B,MAAMI,eAAe,GAAGJ,GAAG,CAAC,IAAI;;IAEhC;IACA,MAAMK,aAAa,GAAIC,IAAI,IAAK;MAC9B,IAAGH,eAAe,CAACI,KAAK,CAACC,MAAM,KAAK,CAAC,EAAE;QACrC;MACF;MACA,MAAM,CAACC,YAAY,CAAC,GAAGR,OAAO,CAACM,KAAK,CAACG,MAAM,CAACJ,IAAI,EAAE,CAAC;MACnDH,eAAe,CAACI,KAAK,CAACI,IAAI,CAACF,YAAY;IACzC;;IAEA;IACA,MAAMG,qBAAqB,GAAIN,IAAI,IAAK;MACtC,MAAM,CAACG,YAAY,CAAC,GAAGN,eAAe,CAACI,KAAK,CAACG,MAAM,CAACJ,IAAI,EAAE,CAAC;MAC3DL,OAAO,CAACM,KAAK,CAACI,IAAI,CAACF,YAAY;MAC/BR,OAAO,CAACM,KAAK,CAACM,IAAI,CAAC,CAACC,CAAC,EAAEC,CAAC,KAAKD,CAAC,CAACE,EAAE,GAAGD,CAAC,CAACC,EAAE;IAC1C;;IAEA;IACA,MAAMC,wBAAwB,GAAGA,CAAA,KAAM;MACrCd,eAAe,CAACI,KAAK,CAACW,OAAO,CAACC,EAAE,IAAI;QAClClB,OAAO,CAACM,KAAK,CAACI,IAAI,CAACQ,EAAE;QA